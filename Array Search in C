//Array Search in C

#include<stdio.h>

void arraysearch(int *arr,int n,int target){
  for(int i=0;i<n;i++){
    if(arr[i]==target){
      printf("%d",i);
        return;
  }
}
printf("-1");

}

int main(){

int n;
printf("enter array size:");
scanf("%d",&n);

int arr[n];
printf("enter array elements:");
 for(int i=0;i<n;i++){
   scanf("%d",&arr[i]);
}

int target;
printf("enter target:");
scanf("%d",&target);

arraysearch(arr,n,target);

return 0;

}
