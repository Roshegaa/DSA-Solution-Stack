//Check for a binary string in C
#include<stdio.h>
#include<string.h>

void binarystr(char *str){
int found=0;
for(int i=0;i<strlen(str);i++){
  if(str[i]=='0'||str[i]=='1'){
    found=1;
}
  else{
    found=0;
      printf("false");
return;
}
}

  if(found==1){
    printf("true");
  }
}

int main(){
char str[100];
printf("enter the input:");
scanf("%s",str);
binarystr(str);
return 0;
}
