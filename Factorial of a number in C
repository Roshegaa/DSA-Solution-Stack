//Factorial of a number in C

#include<stdio.h>

int factorial(int n){
    if(n==0||n==1){
          return 1;
    }
       return n*factorial(n-1);
       
}
int main(){

    int n;
    printf("enter the number:");
    scanf("%d",&n);

   if(n<0){
       printf("no");
   }
   else{
       printf("%d is %d",n,factorial(n));
   }

    return 0;

}
