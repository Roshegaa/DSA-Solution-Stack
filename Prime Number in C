//Prime Number in C

#include<stdio.h>
#include<math.h>

void primenumber(int n){

if(n==0){
  printf("0");
}

else if(n==1){
  printf("1");
}

for(int i=2;i<sqrt(n);i++){
  if(n%i==0){
    printf("NO");
      return;
  }
}

printf("YES");

} 

int main(){

int n;
printf("enter the number:");
scanf("%d",&n);

primenumber(n);

return 0;

}

