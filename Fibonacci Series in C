//Fibonacci Series in C

#include<stdio.h>
#include<math.h>

int fibonacci(int num){
  if(num==0){
   return 0;
 }
  if(num==1){
    return 1;
 }
  return fibonacci(num-1)+fibonacci(num-2);
}

int main(){

int num;
printf("Enter Number:");
scanf("%d",&num);

 for(int i=1;i<=num;i++){
 printf("%d",fibonacci(i));
 }

return 0;

}
