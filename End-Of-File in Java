import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
       String s;
       Scanner sc=new Scanner(System.in);
       int linenum=1;
       while(sc.hasNext()){
          System.out.println(linenum+" "+sc.nextLine());
          linenum++;
       }
       
    }
}
